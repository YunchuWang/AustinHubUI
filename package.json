{
  "name": "austin-hub-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s --proxy-config proxy.conf.json",
    "build": "npm run ng-high-memory build -- --prod",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --prod --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "pretty-quick": "pretty-quick",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.9",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@delon/abc": "^11.10.3",
    "@delon/acl": "^11.10.3",
    "@delon/auth": "^11.10.3",
    "@delon/form": "^11.10.3",
    "@delon/mock": "^11.10.3",
    "@delon/theme": "^11.10.3",
    "@delon/util": "^11.10.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/chart.js": "^2.9.32",
    "ajv": "^7.1.1",
    "angular-bootstrap-md": "^11.0.0",
    "animate.css": "^4.1.1",
    "braintree-web-drop-in": "^1.28.0",
    "canvas-confetti": "^1.4.0",
    "chart.js": "^2.5.0",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "ng-alain": "^11.10.3",
    "ng-zorro-antd": "^11.4.0",
    "ngx-pagination": "^5.1.1",
    "rxjs": "~6.6.0",
    "screenfull": "^5.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.3",
    "ng-alain-codelyzer": "^0.0.1",
    "nz-tslint-rules": "^0.901.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.1",
    "stylelint": "^13.8.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
